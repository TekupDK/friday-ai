{
  "name": "friday-ai",
  "version": "2.0.0",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TekupDK/friday-ai.git"
  },
  "homepage": "https://github.com/TekupDK/friday-ai",
  "bugs": {
    "url": "https://github.com/TekupDK/friday-ai/issues"
  },
  "scripts": {
    "dev": "node check-env.js && dotenv -e .env.dev -- cross-env NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096' tsx watch server/_core/index.ts",
    "dev:vite": "vite --port 5173 --open",
    "dev:tunnel": "node --max-old-space-size=4096 scripts/dev-with-tunnel.mjs",
    "build": "vite build && esbuild server/_core/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "dotenv -e .env.prod -- cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "check:env": "node check-env.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:md": "markdownlint \"**/*.md\"",
    "lint:md:fix": "node scripts/fix-md-ordered-lists.mjs && node scripts/fix-md-hygiene.mjs && npm run lint:md",
    "test:ai": "tsx run-ai-tests.ts",
    "test:ai:conversation": "playwright test tests/ai/friday-ai-agent.test.ts --project=ai-tests",
    "test:ai:visual": "playwright test tests/ai/visual-regression.test.ts --project=ai-tests",
    "test:ai:performance": "playwright test tests/ai/performance.test.ts --project=ai-tests",
    "test:ai:accessibility": "playwright test tests/ai/accessibility.test.ts --project=ai-tests",
    "test:ai:all": "playwright test tests/ai/ --project=ai-tests",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:email-smoke": "dotenv -e .env.dev -- tsx server/scripts/email-smoke-test.ts",
    "test:screens": "playwright test tests/ui-screenshots.spec.ts --project=chromium",
    "db:push": "drizzle-kit push",
    "db:push:dev": "dotenv -e .env.dev -- drizzle-kit push",
    "db:push:prod": "dotenv -e .env.prod -- drizzle-kit push",
    "db:migrate": "drizzle-kit generate && drizzle-kit migrate",
    "db:migrate:dev": "dotenv -e .env.dev -- drizzle-kit generate && dotenv -e .env.dev -- drizzle-kit migrate",
    "db:migrate:prod": "dotenv -e .env.prod -- drizzle-kit generate && dotenv -e .env.prod -- drizzle-kit migrate",
    "migrate:pipeline": "node run-pipeline-migration.mjs",
    "migrate:check": "node scripts/migration-check.mjs --dry-run",
    "migrate:apply": "node scripts/migration-check.mjs --apply",
    "backup:db": "powershell -File scripts/backup-db.ps1 -ConnectionString $env:DATABASE_URL -OutFile backups/db_$(Get-Date -Format yyyyMMdd_HHmmss).sql",
    "migrate:emails": "dotenv -e .env.dev -- tsx server/scripts/migrate-gmail-to-database.ts",
    "migrate:emails:prod": "dotenv -e .env.prod -- tsx server/scripts/migrate-gmail-to-database.ts",
    "migrate:customers": "tsx server/scripts/migrate-customers-from-json.ts",
    "migrate:customers:staging": "tsx server/scripts/migrate-customers-from-json.ts",
    "migrate:customers:prod": "tsx server/scripts/migrate-customers-from-json.ts",
    "crm:seed:dev": "dotenv -e .env.dev -- tsx server/scripts/crm-seed.ts",
    "crm:seed:staging": "dotenv -e .env.staging -- tsx server/scripts/crm-seed.ts",
    "crm:seed:prod": "dotenv -e .env.prod -- tsx server/scripts/crm-seed.ts",
    "crm:seed:templates": "dotenv -e .env.dev -- tsx server/scripts/seed-service-templates.ts",
    "crm:seed:templates:staging": "dotenv -e .env.staging -- tsx server/scripts/seed-service-templates.ts",
    "crm:test:staging": "pnpm exec dotenv -e .env.staging -- vitest run server/__tests__/crm-smoke.test.ts",
    "crm:test:staging:watch": "pnpm exec dotenv -e .env.staging -- vitest watch server/__tests__/crm-smoke.test.ts",
    "crm:cleanup:staging": "pnpm exec dotenv -e .env.staging -- tsx server/scripts/crm-cleanup.ts --dry-run",
    "crm:cleanup:staging:apply": "pnpm exec dotenv -e .env.staging -- tsx server/scripts/crm-cleanup.ts",
    "test:email-smoke:staging": "dotenv -e .env.staging -- tsx server/scripts/email-smoke-test.ts",
    "crm:test:staging:chroma": "cross-env CHROMA_ENABLED=true pnpm exec dotenv -e .env.staging -- vitest run server/__tests__/crm-smoke.test.ts",
    "analyze:customer": "dotenv -e .env.dev -- tsx server/scripts/analyze-emil-laerke.ts",
    "analyze:customer:prod": "dotenv -e .env.prod -- tsx server/scripts/analyze-emil-laerke.ts",
    "logs": "powershell -File scripts/monitor-logs.ps1",
    "logs:ai": "powershell -File scripts/monitor-logs.ps1 -Filter '\\[AI|\\[Chat|\\[LLM'",
    "logs:email": "powershell -File scripts/monitor-logs.ps1 -Filter '\\[Email|\\[Gmail|\\[Calendar'",
    "tunnel:ngrok": "node scripts/tunnel-ngrok.mjs",
    "optimize": "powershell -File scripts/optimize-performance.ps1",
    "optimize:defender": "powershell -ExecutionPolicy Bypass -File scripts/add-defender-exclusions.ps1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:ai-components": "tsx scripts/generate-ai-components-docs.ts",
    "logo:generate": "dotenv -e .env.dev -- tsx server/scripts/generate-gemini-logo.ts",
    "logo:generate:test": "powershell -Command \"(Invoke-WebRequest -Uri https://generativelanguage.googleapis.com/v1beta/models/gemini-2.5-flash:generateContent -Method POST -Headers @{ 'x-goog-api-key'=$env:GEMINI_API_KEY; 'Content-Type'='application/json' } -Body '{\"contents\":[{\"parts\":[{\"text\":\"ping\"}]}]}' ).StatusCode\"",
    "logo:qwen:rendetalje": "python scripts/python/generate_logo.py --company Rendetalje --colors '#00897b,#00acc1' --industry cleaning --style minimalist",
    "logo:qwen:friday": "python scripts/python/generate_logo.py --company 'Friday AI' --colors '#3b82f6,#8b5cf6' --industry 'AI automation' --style futuristic",
    "logo:qwen:tekup": "python scripts/python/generate_logo.py --company Tekup --colors '#8b5cf6,#ec4899' --industry tech --style bold"
  },
