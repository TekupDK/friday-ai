# LiteLLM Environment Variables
# Copy to .env.dev and .env.prod

# OpenRouter API Key (REQUIRED)
OPENROUTER_API_KEY=your-openrouter-api-key-here

# LiteLLM Master Key (optional - for authentication)
LITELLM_MASTER_KEY=friday-litellm-master-key

# LiteLLM Base URL
LITELLM_BASE_URL=http://localhost:4000

# Logging Level
LITELLM_LOG_LEVEL=INFO

# Database URL (optional - leave empty for in-memory)
LITELLM_DATABASE_URL=

# Feature Flag - Enable LiteLLM
ENABLE_LITELLM=false

# Rollout percentage (0-100)
LITELLM_ROLLOUT_PERCENTAGE=0
